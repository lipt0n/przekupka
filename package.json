{
  "name": "makemerich",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_ENV=\"production\" node --max-old-space-size=3072 --max-semi-space-size=64 -r ts-node/register/transpile-only -r tsconfig-paths/register   src/server.ts",
    "startt": "NODE_ENV=\"production\" ts-node -r tsconfig-paths/register --transpile-only  src/server.ts",
    "start:binance": "NODE_ENV=\"production\" PROVIDER=\"binance\" ts-node -r tsconfig-paths/register --transpile-only  src/server.ts",
    "dev": "node --inspect=0.0.0.0:9229 --trace-warnings -r ts-node/register -r tsconfig-paths/register src/server.ts",
    "dev-server": "nodemon",
    "ui": "parcel  public/index.html",
    "build": "parcel build public/index.html",
    "deploy": "parcel build public/index.html &&  ./syncWithServer.sh",
    "test": "jest",
    "init:hitbtc": "ts-node -r tsconfig-paths/register ./src/api/hitBtc/create-initial-pairs.ts",
    "init:binance": "ts-node -r tsconfig-paths/register ./src/api/binance/create-initial-pairs.ts",
    "cleanup:binance": "ts-node -r tsconfig-paths/register --transpile-only ./src/api/binance/cleanup-to-sell.ts",
    "stepup": "ts-node -r tsconfig-paths/register ./src/utils/step-up-prices.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "e": "NODE_ENV=\"production\" node --max-old-space-size=3072 --max-semi-space-size=64 -r ts-node/register/transpile-only -r tsconfig-paths/register   src/experiment.ts",
    "sellOld": "NODE_ENV=\"production\" node --max-old-space-size=3072 --max-semi-space-size=64 -r ts-node/register/transpile-only -r tsconfig-paths/register   src/api/binance/sell-old.ts"
  },
